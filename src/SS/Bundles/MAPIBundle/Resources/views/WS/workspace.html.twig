{% extends 'SSMAPIBundle::Application/workspace.html.twig' %}

{% block bundleHeader %}
	<a
		class="ui-widget-header"
		href="{{ path('mapi_ws_bundle', {
			'bundle': bundleName
		}) }}">
		{{ bundleName }}
	</a>
{% endblock %}

{% block bundleFooter %}
	<div class="ui-widget-footer ui-buttonset">

		<a href="{{ path('mapi_ws_bundle',{'bundle': bundleName}) }}">
			{% trans %}Open{% endtrans %}
		</a>

		<a href="{{ path('mapi_ws_wsdl',{'bundle': bundleName}) }}">
			{% trans %}WSDL{% endtrans %}
		</a>

		<a href="{{ path('mapi_ws_soap',{'bundle': bundleName}) }}">
			{% trans %}SOAP{% endtrans %}
		</a>

		<a href="{{ path('mapi_ws_rest',{'bundle': bundleName}) }}">
			{% trans %}REST{% endtrans %}
		</a>

		<a href="{{ path('mapi_ws_json',{'bundle': bundleName}) }}">
			{% trans %}JSON{% endtrans %}
		</a>
	</div>
{% endblock %}