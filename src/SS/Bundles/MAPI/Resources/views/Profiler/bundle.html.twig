{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
	{% set icon %}
	<img
		width="16"
		height="28"
		alt="SOAP"
		src="data:image/png;base64," />
	{% if collector.getSoap|length == 0 %}
		<span class="sf-toolbar-status sf-toolbar-status-red">0</span>
	{% else %}
		<span class="sf-toolbar-status sf-toolbar-status-green">
			{{ collector.getSoap|length }}
		</span>
	{% endif %}
	{% endset %}

	{% set text %}
		<div class="sf-toolbar-info-piece">
			<b>SOAP calls:</b> <span>{{ collector.getSoap|length }}</span>
		</div>
	{% endset %}

	{% include '@WebProfiler/Profiler/toolbar_item.html.twig' with {
		'link': profiler_url
	} %}
{% endblock %}

{% block menu %}
	<span class="label">
		<span class="icon">
			<img
				src="data:image/png;base64,"
				alt="SOAP" />
		</span> 
		<strong>Bundle</strong>
	</span>
{% endblock %}

{% block panel %}
	{% if collector.getSoap == false %}
		<p>
			<em>No Bundle</em>
		</p>
	{% else %}

	{% for soapCall in collector.getSoap %}
<h2>Service: {{ soapCall.soapName }}</h2>
<h3>SOAP Request</h3>
<h3>Headers</h3>
<p>
<pre>{{ soapCall.requestHeaders }}</pre>
</p>
<br />
<h3>Body</h3>
{% if soapCall.soapRequest == false %}
<p>
	<em>No SOAP request</em>
</p>
{% elseif soapCall.soapRequest %}
<pre>
                    {{ soapCall.soapRequest }}
                </pre>
{% endif %}

<h3>SOAP Response</h3>
<h3>Headers</h3>
<p>
<pre>{{ soapCall.responseHeaders }}</pre>
</p>
<h3>Body</h3>
{% if soapCall.soapResponse == false %}
<p>
	<em>No SOAP response</em>
</p>
{% elseif soapCall.soapResponse %}
<pre>
                    {{ soapCall.soapResponse }}
                </pre>
{% endif %} {% endfor %} {% endif %} {% endblock %}
