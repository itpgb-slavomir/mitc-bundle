{% extends 'SSMAPIBundle::Application/Bundle/toolbar.html.twig' %}

{% block toolbarButtonSet %}

	{% set bundleReflection=model.bundle %}
	{% set bundle=bundleReflection.getBundle() %}
	{% set namespace=model.namespace %}

	<h2>
		{% trans %}Namespace{% endtrans %}
	</h2>

	<a href="{{ path('mapi_application_bundle', {
			'bundle':bundle.getName(),
			'namespace':namespace.getName()
		}) }}">
		{{ namespace.getName() }}
	</a>

	{{ parent() }}

{% endblock %}